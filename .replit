[run]
command = "python app.py"

[env]
PYTHONPATH = "/home/runner/$REPL_SLUG"
PYTHONUNBUFFERED = "1"
FLASK_APP = "app.py"
FLASK_ENV = "development"
PORT = "5000"

[features]
search = true

[packager]
ignoredPackages = ["python2.7", "python2"]

[debugger]
support = true
interpreter = "python3"

[languages]

[languages.python]
pattern = "**/*.py"

[languages.python.languageServer]
start = "pylsp"